require 'spec_helper'

describe CacheStatistic do
  describe 'extracting data from the database' do
    it 'should return the correct data from the database'
    it 'should return the data in order of date'
    it 'should return an empty list if there is nothing in the database'
  end
  describe 'extracting the date from the database' do
    it 'should return the correct data from the database'
    it 'should return the data in order of date'
    it 'should return an empty list if there is nothing in the database'
  end
  describe 'extracting total number of users from the database' do
    it 'should return the correct data from the database'
    it 'should return the data in order of date'
    it 'should return an empty list if there is nothing in the database'
  end
  describe 'extracting server load from the database' do
    it 'should return the correct data from the database'
    it 'should return the data in order of date'
    it 'should return an empty list if there is nothing in the database'
  end
  describe 'extracting server load over number of users from the database' do
    it 'should return the correct data from the database'
    it 'should return the data in order of date'
    it 'should return an empty list if there is nothing in the database'
  end
end